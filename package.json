{
  "name": "engage-microservice",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' nodemon src/server.ts",
